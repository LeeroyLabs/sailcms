ENVIRONMENT=dev

SITE_URL=http://localhost
DEBUG=off

# Composer location
COMPOSER_LOCATION=/usr/local/bin/composer

# Output Compression
USE_COMPRESSION=false

# Queue System
QUEUE_MAX_PROCESS_PER_RUN=all
QUEUE_MAX_RETRY=3

# Database
DATABASE_DSN="mongodb://localhost:27017/sailcms"
DATABASE_DB="sailcms"

# Caching
CACHE_USE=false
CACHE_USER=""
CACHE_PASSWORD=""
CACHE_HOST=localhost